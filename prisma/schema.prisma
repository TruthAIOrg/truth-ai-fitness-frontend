generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model tb_res_en {
  id        Int     @id @default(autoincrement())
  part_name String
  model_id  String
  item_name String
  tag_text  String?
  video_url String?
  content   String?
}

model tb_res_zh {
  id        Int     @id @default(autoincrement())
  part_name String
  model_id  String
  item_name String
  tag_text  String?
  video_url String?
  content   String?
}
